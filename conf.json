{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/)"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "systemName": "Tim-resource",
    "footer": "",
    "copyright": "Copyright Â© Timewax",
    "includeDate": true,
    "navType": "vertical",
    "theme": "paper",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": true,
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "dateFormat": "ddd MMM Do YYYY",
    "sort": true,
    "search": true,
    "syntaxTheme": "dark"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "lenient": true,
    "template": "./node_modules/ink-docstrap/template"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
